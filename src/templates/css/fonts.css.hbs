/*
	Tiempos font on asset library
*/
@font-face {
	font-family: 'tiempos-headline';
	font-weight: 600;
	font-style: normal;
	src: url("{{fontPath}}tiempos-headline-web-semibold.woff2") format("woff2"), url("{{fontPath}}tiempos-headline-web-semibold.woff") format("woff"), url("{{fontPath}}tiempos-headline-web-semibold.eot") format("embedded-opentype")
}
