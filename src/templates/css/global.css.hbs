body, html {
	background-color: #D4D4D4;
	border: 0 none;
	overflow: hidden;
	outline: 0;
	margin: 0;
	padding: 0;
	position: absolute;
}
html {
	font-family: proxima-nova, Arial, sans-serif;
	font-weight: 400;
	font-style: normal;
}
div, h1, p {
	font-family: proxima-nova, Arial, sans-serif;
	font-weight: 600;
	font-style: normal;
	/* Adjust font size */
	font-size: 100%;
	-webkit-text-size-adjust: 100%;	
	/* Font varient */
	font-variant-ligatures: none;
	-webkit-font-variant-ligatures: none;
	/* Smoothing */
	font-smoothing: antialiased;
	text-rendering: optimizeLegibility;
	-moz-osx-font-smoothing: grayscale;
	-webkit-font-smoothing: antialiased;
	margin:0;
}
button.abs {
	background-color: Transparent;
	background-repeat: no-repeat;
	border: none;
	cursor: pointer;
	overflow: hidden;
	outline: none;
}
button.abs:focus, .fake-button:focus {
	outline: 5px solid #e85b16;
}
sup{
	font-size: 8px;
	line-height: 50% !important;
	vertical-align: baseline;
	position: relative;
	top: -0.9em;{{!--
	font-size: 50%;/*75%;*/
	line-height: 50% !important;
	vertical-align: baseline;
	position: relative;
	top: -0.6em;/* -0.4em;*/--}}
}

.abs, img, svg {
	position:absolute;
	display:block;
}
.border {
	border: 1px solid var(--coolgray8);
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	position: absolute;
	pointer-events: none
}
{{!.clickable {cursor: pointer} }}
.hide {visibility: hidden}
.full-size {
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	width: {{width}}px;
	height: {{height}}px;
}
#banner {
	background-color: var(--vibrantblue);
	visibility: hidden;
	position: relative;
	overflow: hidden;
}
